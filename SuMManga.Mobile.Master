                            if (IMGKey == 'Creator') {
                                SuMLoadingSVGPreviewElmF0C0.innerHTML = SuMCreatorSVGPath;
                            }
                        };

                        var SuMCoinsReq = 0;
                        var CuurCoinsCountF0CGSF = 0;

                        function SuMReqCoinsCountFU(FMID, callback) {
                            document.getElementById('CoinsCountF0CMF').innerText = '...';
                            $.ajax({
                                dataType: "json",
                                method: 'post',
                                url: '<%= ResolveUrl("BTNTesting.aspx/GetMangaCoinsCountFSQL") %>',
                                data: JSON.stringify({ MID: FMID }),
                                contentType: "application/json; charset=utf-8",
                                success: function (msg) {
                                    document.getElementById('CoinsCountF0CMF').innerText = '';
                                    $("#CoinsCountF0CMF").append(msg.d);
                                    SuMCoinsReq = msg.d;
                                    callback();
                                },
                                error: function (e) {
                                    document.getElementById('CoinsCountF0CMF').innerText = '';
                                    $("#CoinsCountF0CMF").append('!');
                                    SuMCoinsReq = '!';
                                    callback();
                                }
                            });
                        };

                        function SuMCoinsCountFU(FUID) {
                            $.ajax({
                                dataType: "json",
                                method: 'post',
                                url: '<%= ResolveUrl("BTNTesting.aspx/GetCoinsCountFSQL") %>',
                                data: JSON.stringify({ UID: FUID }),
                                contentType: "application/json; charset=utf-8",
                                success: function (msg) {
                                    $("#CuurCoinsCountF0CGSF").append(msg.d);
                                    CuurCoinsCountF0CGSF = msg.d;
                                },
                                error: function (e) {
                                    $("#CuurCoinsCountF0CGSF").append('!');
                                    CuurCoinsCountF0CGSF = null;
                                }
                            });
                        };

                        function SuMCoinMasterCard(HerfLink, CurrUIDF0CGF, HerfInfo, HerfThemeColor) {
                            SuMLoadingIsInPross = false;
                            if (CuurCoinsCountF0CGSF >= SuMCoinsReq) {
                                SuMUpdateCoinsCount(CurrUIDF0CGF, SuMCoinsReq * (-1));
                                SuMGoToThis(HerfLink, HerfThemeColor, HerfInfo, 'ContantExplorer');
                            } else {
                                SuMGoToThis('/AccountETC/Settings.aspx?TC=var(--SuMDWhiteOP100)', 'var(--SuMThemeColorOP74)', 'SuM Settings', 'Settings');
                            }
                        };

                        function SuMCoinMasterCloseAllCS() {
                            SuMLoadingIsInPross = false;
                            document.getElementById('LoadingSuMInfoBub').style.display = 'none';
                            document.getElementById('SuMUnlockCancelDiv').style.display = 'none';
                        };

                        /* SuM Coins Count FixUP */
                        var CurrUIDF0CFixUp0 = getUIDFrUserCo();
                        SuMCoinsCountFU(CurrUIDF0CFixUp0);

                        function SuMGoToThis(HerfLink, HerfThemeColor, HerfInfo, IMGID) {
                            if (SuMLoadingIsInPross == false && HerfLink.includes(document.URL) == false && document.URL.includes(HerfLink) == false) {
                                SuMLoadingIsInPross = true;
                                SuMLoadingAnConElmF0C0.style.display = 'block';
                                SuMLoadingFailedTXTElm.style.display = 'none';
                                document.getElementById('SuMUnlockCancelDiv').style.display = 'none';
                                document.getElementById('SuMShowCoinMCardBTN').style.display = 'none';
                                document.getElementById('SuMCoinUnlockBTN').removeAttribute('onclick');
                                document.getElementById('SuMCoinUnlockBTN').innerText = 'loading...';
                                SuMLoadHerfFailed = false;
                                if (HerfThemeColor.includes('--') == false) {
                                    SuMLoadingTitleElm.style.color = HerfThemeColor.replace('0.74', '0.92');
                                    SuMLoadingAnConElmF0C0.style.backgroundColor = HerfThemeColor.replace('0.74', '0.18');
                                    SuMLoadingAnElmF0C0.style.backgroundColor = HerfThemeColor.replace('0.74', '0.92');
                                    SuMLoadingSVGPreviewElmF0C0.setAttribute('fill', HerfThemeColor.replace('0.74', '0.92'));
                                    SuMLoadingFailedTXTElm.style.color = HerfThemeColor.replace('0.74', '0.64');
                                    document.getElementById('SuMCoinUnlockBTN').style.backgroundColor = HerfThemeColor.replace('0.74', '0.86');
                                }
                                else {
                                    SuMLoadingTitleElm.style.color = 'var(--SuMThemeColorOP92)';
                                    SuMLoadingAnConElmF0C0.style.backgroundColor = 'var(--SuMThemeColorOP14)';
                                    SuMLoadingAnElmF0C0.style.backgroundColor = 'var(--SuMThemeColorOP92)';
                                    SuMLoadingSVGPreviewElmF0C0.setAttribute('fill', 'var(--SuMThemeColorOP92)');
                                    SuMLoadingFailedTXTElm.style.color = 'var(--SuMThemeColorOP64)';
                                    document.getElementById('SuMCoinUnlockBTN').style.backgroundColor = 'var(--SuMThemeColorOP86)';
                                }
                                SuMLoadingPIMGLoad(IMGID);
                                SuMLoadingTitleElm.innerText = HerfInfo;
                                SuMLoadingContElm.style.display = 'block';
                                if (navigator.onLine == true) {
                                    if (IMGID == 'ContantExplorer' || IMGID == 'Creator') {
                                        ExpandTranStartF0C0(HerfThemeColor);
                                        SuMActivateAnimationF0C0();
                                    } else {
                                        if (IMGID != 'MangaExplorer') {
                                            SuMActivateAnimationF0C0();
                                        }
                                    }
                                    if (IMGID != 'MangaExplorer') {
                                        setTimeout(() => {
                                            if (HerfLink != null) {
                                                if (document.URL.includes('/MangaExplorer.aspx') == true) {
                                                    HideMangaExplorerTopPartBar();
                                                    HideMangaExplorerBar();
                                                }
                                                if (document.URL.includes('/MangaExplorer.aspx') == false && HerfLink.includes('/MangaExplorer.aspx') == true) {
                                                    SuMHideFullNavAnF0C0();
                                                }
                                                SuMUltraCloseCard();
                                                setTimeout(() => {
                                                    location.href = HerfLink;
                                                }, 360);
                                            }
                                        }, 840);
                                    } else {
                                        //-sumcoinpross
                                        document.getElementById('SuMShowCoinMCardBTN').style.display = 'flex';
                                        SuMLoadingAnConElmF0C0.style.display = 'none';
                                        var FDMID = 0;
                                        var CurrUIDF0CGF = getUIDFrUserCo();
                                        var MIDEXTRaw = HerfLink.split('&');
                                        for (var ic = 0; ic < MIDEXTRaw.length; ic++) {
                                            if (MIDEXTRaw[ic].includes('VC=') == true) {
                                                FDMID = MIDEXTRaw[ic].replace('VC=', '').replace(' ', '');
                                                ic = MIDEXTRaw;
                                            }
                                        }
                                        SuMReqCoinsCountFU(FDMID, function () {

                                            SuMCoinsCountFU(CurrUIDF0CGF);
                                            document.getElementById('CoinsCountF0CMF').innerText = SuMCoinsReq;
                                            document.getElementById('SuMCoinUnlockBTN').setAttribute('onclick', "SuMCoinMasterCard('" + HerfLink + "'," + CurrUIDF0CGF + ",'" + HerfInfo + "','" + HerfThemeColor + "');");
                                            document.getElementById('SuMUnlockCancelDiv').style.display = 'block';
                                            if (CuurCoinsCountF0CGSF >= SuMCoinsReq) {
                                                document.getElementById('SuMCoinUnlockBTN').innerText = 'Unlock!';
                                            } else {
                                                document.getElementById('SuMCoinUnlockBTN').innerText = 'Get Coins';
                                            }

                                        });
                                    }
                                }
                                else {
                                    if (IMGID != 'MangaExplorer') {
                                        fetch(HerfLink, { method: 'GET' }).then(res => {
                                            SuMActivateAnimationF0C0();
                                            if (IMGID == 'ContantExplorer' || IMGID == 'Creator') {
                                                ExpandTranStartF0C0(HerfThemeColor);
                                            }
                                            setTimeout(() => {
                                                if (HerfLink != null) {
                                                    if (document.URL.includes('/MangaExplorer.aspx') == true) {
                                                        HideMangaExplorerTopPartBar();
                                                        HideMangaExplorerBar();
                                                    }
                                                    if (document.URL.includes('/MangaExplorer.aspx') == false && HerfLink.includes('/MangaExplorer.aspx') == true) {
                                                        SuMHideFullNavAnF0C0();
                                                    }
                                                    SuMUltraCloseCard();
                                                    setTimeout(() => {
                                                        location.href = HerfLink;
                                                    }, 360);
                                                }
                                            }, 840);
                                        }).catch(err => {
                                            SuMLoadingAnConElmF0C0.style.display = 'none';
                                            SuMLoadingFailedTXTElm.style.display = 'block';
                                            setTimeout(() => {
                                                HideSuMLoadingPopUp();
                                                SuMLoadingIsInPross = false;
                                            }, 2040);
                                        });
                                        SuMLoadHerfFailed = true;
                                    } else {
                                        //--sumcoainpross -fail
                                        SuMLoadingAnConElmF0C0.style.display = 'none';
                                        SuMLoadingFailedTXTElm.style.display = 'block';
                                        setTimeout(() => {
                                            HideSuMLoadingPopUp();
                                            SuMLoadingIsInPross = false;
                                        }, 2040);
                                    }
                                }
                            }
                        };

                        function SuMGoBack() {
                            var HerfLink = document.referrer;
                            var HerfThemeColor = null;
                            var HerfInfo = null;
                            var IMGID = null;
                            if (HerfLink.includes('Explore.aspx') == true) {
                                IMGID = 'Explore';
                                HerfInfo = 'SuM Explore';
                            }
                            if (HerfLink.includes('Hits.aspx') == true) {
                                IMGID = 'Hits';
                                HerfInfo = 'SuM Hits';
                            }
                            if (HerfLink.includes('ContantExplorer.aspx') == true) {
                                IMGID = 'ContantExplorer';
                            }
                            if (HerfLink.includes('MangaExplorer.aspx') == true) {
                                IMGID = 'MangaExplorer';
                            }
                            if (HerfLink.includes('Settings.aspx') == true) {
                                IMGID = 'Settings';
                                HerfInfo = 'SuM Settings';
                            }
                            if (HerfLink.includes('UserLibrary.aspx') == true) {
                                IMGID = 'UserLibrary';
                                HerfInfo = 'My Library';
                            }
