                        function SuMLoadingPIMGLoad(IMGKey) {
                            if (IMGKey == 'MangaExplorer' || IMGKey == 'ContantExplorer') {
                                SuMLoadingSVGPreviewElmF0C0.innerHTML = SuMMangaNContantExpSVGPath;
                            }
                            if (IMGKey == 'Explore') {
                                SuMLoadingSVGPreviewElmF0C0.innerHTML = SuMExploreSVGPath;
                            }
                            if (IMGKey == 'Settings') {
                                SuMLoadingSVGPreviewElmF0C0.innerHTML = SuMSettingsSVGPath;
                            }
                            if (IMGKey == 'UserLibrary') {
                                SuMLoadingSVGPreviewElmF0C0.innerHTML = SuMUserLibrarySVGPath;
                            }
                            if (IMGKey == 'Hits') {
                                SuMLoadingSVGPreviewElmF0C0.innerHTML = SuMHitsSVGPath;
