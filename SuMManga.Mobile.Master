            };
            if (document.getElementById('subnavscont2').style.display == 'block') {
                document.getElementById('fullnavscont').style.display = 'none';
            };
            
            if (!navigator.onLine) {
                document.getElementById('FullSearch').onclick = "document.getElementById('SearchOffline').style.display = 'block'";
                document.getElementById('SubSearch').onclick = "document.getElementById('SearchOffline').style.display = 'block'";
                document.getElementById('MainContent_AccountSettingsOrLogin').href = '#';
                document.getElementById('SendCommentAria').style.display = 'none';
            };
            if ("androidAPIs" in window == false) {

                location.href = '/SuMMangaInstallAPP.aspx';
            }
            var MangaExplorerBarElm = document.getElementById('<%= subnavscont2.ClientID %>');
            function HideMangaExplorerBar() {
                if (MangaExplorerBarElm.classList.contains('fadeInUp') == true || MangaExplorerBarElm.classList.contains('fadeOutDown') == false) {
                    MangaExplorerBarElm.classList.remove('fadeInUp');
                    if (MangaExplorerBarElm.classList.contains('fadeOutDown') == false) {
                        MangaExplorerBarElm.classList.add('fadeOutDown');
                    }
                    setTimeout(() => {
                        MangaExplorerBarElm.style.display = 'none';
                    }, 360);
                }
            };
            function ShowMangaExplorerBar() {
                if (MangaExplorerBarElm.classList.contains('fadeInUp') == false || MangaExplorerBarElm.classList.contains('fadeOutDown') == true) {
                    MangaExplorerBarElm.classList.remove('fadeOutDown');
                    if (MangaExplorerBarElm.classList.contains('fadeInUp') == false) {
                        MangaExplorerBarElm.classList.add('fadeInUp');
                    }
                    MangaExplorerBarElm.style.display = 'block';
                }
            };
            function NextBtnOrState() {
                document.getElementById('MainContent_NextChapter').style.marginTop = (window.innerHeight - document.getElementById('subnavscont2').offsetHeight - document.getElementById('MainContent_NextChapter').offsetHeight - 6) + 'px';
            };
            function NextBtnFullSState() {
                document.getElementById('MainContent_NextChapter').style.marginTop = (window.innerHeight - document.getElementById('MainContent_NextChapter').offsetHeight - 12) + 'px';
            }
        </script>
